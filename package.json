{
  "name": "free-designer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "lib-flexible": "^0.3.2",
    "mint-ui": "^2.2.13",
    "muse-ui": "^3.0.1",
    "muse-ui-loading": "^0.2.0",
    "normalize.css": "^8.0.0",
    "postcss-pxtorem": "^4.0.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 75,
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "mu-picker",
          "mu-date",
          "mu-day",
          "mu-icon-button",
          "mint-popup",
          "mint-msgbox"
        ]
      }
    }
  },
  "vue": {
    "devServer": {
      "proxy": {
        "/api/": {
          "target": "http://180.76.53.122:8083",
          "changeOrigin": true,
          "cookieDomainRewrite": "www.free-designer.cn",
          "headers": {
            "Referer": "http://www.free-designer.cn"
          }
        }
      }
    }
  }
}
